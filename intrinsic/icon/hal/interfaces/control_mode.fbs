namespace intrinsic_fbs;

// Enum of potential motor control modes.
enum ControlMode : uint8 {
  kUnknown = 0,
  kCyclicPosition = 1,
  kCyclicVelocity = 2,
  kCyclicTorque = 3,
  kHandguiding = 4,
}

// Message that contains the ControlMode status.
table ControlModeStatus {
  status : ControlMode;
}
